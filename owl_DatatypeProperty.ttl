p-lod:sort-order-label a owl:DatatypeProperty ;
    rdfs:label "Sort Order Label" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:surface-area a owl:DatatypeProperty ;
    rdfs:label "Surface Area (in meters)" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .
