@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p-lod: <urn:p-lod:id:> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

p-lod:arcgis-id a owl:DatatypeProperty ;
    rdfs:label "ArcGIS Id" ;
    rdfs:comment "The identifier of the object in the ArcGIS files generated by the Pompeii Bibliography and Mapping Project" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:description a owl:DatatypeProperty ;
    rdfs:label "Description" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:plod-english-title a owl:DatatypeProperty ;
    rdfs:label "English Title" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:geojson a owl:DatatypeProperty ;
    rdfs:label "Geojson String" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:has-action a owl:DatatypeProperty ;
    rdfs:label "Has action" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:identifier a owl:DatatypeProperty ;
    rdfs:label "Identifier" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:is-plural a owl:DatatypeProperty ;
    rdfs:label "Is plural" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:plod-italian-title a owl:DatatypeProperty ;
    rdfs:label "Italian Title" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:length a owl:DatatypeProperty ;
    rdfs:label "Length (meters)" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:nam-inventory-number a owl:DatatypeProperty ;
    rdfs:label "NAM Inventory Number" ;
    rdfs:comment "The inventory number of the National Archaeological Museum of Naples" ;
    rdfs:isDefinedBy <https://p-lod.github.io/p-lod-data/owl_DatatypeProperty.ttl> .

p-lod:other-collection-inventory-number a owl:DatatypeProperty ;
    rdfs:label "Other Collection Inventory Number" ;
